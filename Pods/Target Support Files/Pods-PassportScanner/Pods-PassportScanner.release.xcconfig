ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
CLANG_CXX_LIBRARY = compiler-default
CLANG_MODULES_AUTOLINK = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/EVGPUImage2" "${PODS_CONFIGURATION_BUILD_DIR}/GPUImage" "${PODS_CONFIGURATION_BUILD_DIR}/TesseractOCRSDKiOS" "${PODS_CONFIGURATION_BUILD_DIR}/UIImage-Resize"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/EVGPUImage2/EVGPUImage2.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/GPUImage/GPUImage.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/TesseractOCRSDKiOS/TesseractOCRSDKiOS.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/UIImage-Resize/UIImage_Resize.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -l"stdc++" -l"z" -framework "EVGPUImage2" -framework "GPUImage" -framework "TesseractOCRSDKiOS" -framework "UIImage_Resize"
OTHER_SWIFT_FLAGS = $(inherited) -DGLES $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
